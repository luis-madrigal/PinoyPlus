<!DOCTYPE html>
<html>

<head>
	<title>PinoyPlus</title>

	<!-- Font Awesome. Used for icons -->
	<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
	<!-- jQuery library -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<!-- Popper JS -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
	<!-- Latest compiled JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
	<!-- Strophe XMPP -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/strophe.js/1.2.15/strophe.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/muc/strophe.muc.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/roster/strophe.roster.js"></script>

	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/rsm/strophe.rsm.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.6/mam/strophe.mam.v0.3.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/archive/strophe.archive.js"></script>


	<!-- Vue.js development version, includes helpful console warnings -->
	<!-- <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script> -->

	<!-- <script src=""></script> -->
	<!-- Chart.js -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>

	<!--W3 CSS-->
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" href="css/chatbox.css">
	<link rel="stylesheet" href="css/global.css">
	<link rel="stylesheet" href="css/navbar.css">
	<link rel="stylesheet" href="css/footer.css">
	<link rel="stylesheet" href="css/dashboard.css">
	<link rel="stylesheet" href="css/calendar.css">
</head>

<body>

	<% include partials/chat %>

	<% include partials/navbar %>
	<div class="container container-with-spacing">
		<div class="row">
			<div class="col-lg-8">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="red-rect-bg welcome">
							<p class="welcome-text">"Welcome back, PLHIV Response Center"</p>
							<p class="welcome-msg-count">You have

								<b class="link-glow" onclick="w3_open()">5</b>
								new messages</p>
							<!-- Welcom back caloocan city -->
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-lg-6">
						<div class="row row-with-padding">
							<div class="col-lg-12">
								<div class="white-rect-bg">
									<div class="container">
										<div class="row">
											<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
												<p class="box-label">Calendar &amp; Events</p>
												<div id="my-calendar"></div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="event-list">
						</div>
					</div>
					<div class="col-lg-6">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Cases Status Report</p>
										<p class="chart-title">Referred Services</p>
										<canvas id="refServicesChart" height="170vh"></canvas>
										<p class="chart-title">Number of Cases</p>
										<canvas id="numCasesChart" height="177"></canvas>
									</div>
								</div>
							</div>
						</div>
						<!-- case status report -->
					</div>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Latest Updates</p>
										<div class="update-list">
										</div>
									</div>
								</div>
							</div>

						</div>
					</div>
				</div>
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Announcement Notifications</p>
										<div class="announcement-list">
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="modal fade" id="eventModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog modal-lg" role="document">
				<div class="modal-content">
					<div class="modal-header bg-red">
						<h5 class="modal-title" id="exampleModalLabel">Add an event</h5>
						<button type="button" class="custom-close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<div class="modal-body bg-white">
						<div class="container">
							<div class = "row">
								<div class = "col-sm-12">
									<p id = "dateLabel">August 18, 2018</p>
								</div>
							</div>
							<div class = "row">
								<div class = "col-sm-10">
									<p class = "instructions">Please complete the information about the event or activity by filling out the boxes below.</p>
								</div>
								<div class = "col-sm-2"></div>
							</div>
							<div class="row">
								<div class = "col-lg-12">
									<form>
										<div class = "row">
											<div class = "col-sm-2">
												<label for = "name">Event/Activity:</label>
											</div>
											<div class = "col-sm-6">
												<input type = "text" class = "form-control" id = "name">
											</div>
											<div class = "col-sm-4"></div>
										</div>
										<div class = "row">
											<div class = "col-sm-2">
												<label for = "hour">Time:</label>
											</div>
											<div class = "col-sm-2">
												<input type = "number" class = "form-control" id = "hour">
											</div>
											<div class = "col-sm-1">
												<span id = "divider">:</span>
											</div>
											<div class = "col-sm-2">
												<input type = "number" class = "form-control" id = "minute">
											</div>
											<div class = "col-sm-1">
												<select class = "form-control" id = "ampm">
													<option>AM</option>
													<option>PM</option>
												</select>
												<span id = "ampmCaret" class="fa fa-caret-down"></span>
											</div>
											<div class = "col-sm-4"></div>
										</div>
										<div class = "row">
											<div class = "col-sm-2">
												<label for = "venue">Venue:</label>
											</div>
											<div class = "col-sm-6">
												<input type = "text" class = "form-control" id = "venue">
											</div>
											<div class = "col-sm-4"></div>
										</div>
										<div class = "row">
											<div class = "col-sm-2">
												<label for = "notes">Notes:</label>
											</div>
											<div class = "col-sm-6">
												<textarea class = "form-control" id = "notes"></textarea>
											</div>
											<div class = "col-sm-4"></div>
										</div>
										<div class="row">
											<div class="col-sm-10">
											</div>
											<div class = "col-sm-2">
												<div class = "save-btn red-rect-bg">
													<p class="center">Save</p>
												</div>
											</div>
										</div>
									</form>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<% include partials/footer %>

	<input hidden id="username" value="<%= username %>">
	<input hidden id="password" value="<%= password %>">
	<input hidden id="adminAccount" value="<%= adminAccount %>">
	<input hidden id="adminServerUrl" value="<%= adminServerUrl %>">
	<input hidden id="chatServerUrl" value="<%= chatServerUrl %>">
	<input hidden id="chatHost" value="<%= chatHost %>">
</body>


<script src="js/keycodes.js"></script>
<script src="js/admin.js"></script>
<script src="js/dashboard-chat.js"></script>
<script src="js/edit-profile.js"></script>
<script src="js/calendar.js"></script>
<script src="js/dashboard.js"></script>
<script src="js/charts.js"></script>
<script src="js/messagebox.js"></script>
<script src="js/navbar.js"></script>

</html>