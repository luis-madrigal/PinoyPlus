<!DOCTYPE html>
<html>

<head>
	<title>PinoyPlus</title>

	<!-- Font Awesome. Used for icons -->
	<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
	<!-- jQuery library -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<!-- Popper JS -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
	<!-- Latest compiled JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
	<!-- Strophe XMPP -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/strophe.js/1.2.15/strophe.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/muc/strophe.muc.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/roster/strophe.roster.js"></script>

	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/rsm/strophe.rsm.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.6/mam/strophe.mam.v0.3.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/archive/strophe.archive.js"></script>


	<!-- Vue.js development version, includes helpful console warnings -->
	<!-- <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script> -->

	<!-- <script src=""></script> -->
	<!-- Chart.js -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>

	<!--W3 CSS-->
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" href="css/chatbox.css">
	<link rel="stylesheet" href="css/global.css">
	<link rel="stylesheet" href="css/navbar.css">
	<link rel="stylesheet" href="css/footer.css">
	<link rel="stylesheet" href="css/dashboard.css">
	<link rel="stylesheet" href="css/calendar.css">
</head>

<body>

	<% include partials/chat %>

	<% include partials/navbar %>
	<div class="container container-with-spacing">
		<div class="row">
			<div class="col-lg-8">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="red-rect-bg welcome">
							<p class="welcome-text">"Welcome back, PLHIV Response Center"</p>
							<p class="welcome-msg-count">You have

								<b class="link-glow" onclick="w3_open()">5</b>
								new messages</p>
							<!-- Welcom back caloocan city -->
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-lg-6">
						<div class="row row-with-padding">
							<div class="col-lg-12">
								<div class="white-rect-bg">
									<div class="container">
										<div class="row">
											<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
												<p class="box-label">Calendar &amp; Events</p>
												<div id="my-calendar"></div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="event-list">
						</div>
					</div>
					<div class="col-lg-6">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Cases Status Report</p>
										<p class="chart-title">Referred Services</p>
										<canvas id="refServicesChart" height="170vh"></canvas>
										<p class="chart-title">Number of Cases</p>
										<canvas id="numCasesChart" height="177"></canvas>
									</div>
								</div>
							</div>
						</div>
						<!-- case status report -->
					</div>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Latest Updates</p>
										<div class="update-list">
										</div>
									</div>
								</div>
							</div>

						</div>
					</div>
				</div>
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Announcement Notifications</p>
										<div class="announcement-list">
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<% include partials/footer %>

	<input hidden id="username" value="<%= username %>">
	<input hidden id="password" value="<%= password %>">
	<input hidden id="adminAccount" value="<%= adminAccount %>">
	<input hidden id="adminServerUrl" value="<%= adminServerUrl %>">
	<input hidden id="chatServerUrl" value="<%= chatServerUrl %>">
	<input hidden id="chatHost" value="<%= chatHost %>">
</body>


<script src="js/keycodes.js"></script>
<script src="js/admin.js"></script>
<script src="js/dashboard-chat.js"></script>
<script src="js/edit-profile.js"></script>
<script src="js/calendar.js"></script>
<script src="js/dashboard.js"></script>
<script src="js/charts.js"></script>
<script src="js/messagebox.js"></script>
<script src="js/navbar.js"></script>

</html>