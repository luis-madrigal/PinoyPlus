<!DOCTYPE html>
<html>

<head>
	<title>PinoyPlus</title>

	<!-- Font Awesome. Used for icons -->
	<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
	<!-- Latest compiled and minified CSS -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
	<!-- jQuery library -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
	<!-- Popper JS -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
	<!-- Latest compiled JavaScript -->
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
	<!-- Strophe XMPP -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/strophe.js/1.2.15/strophe.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/muc/strophe.muc.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/roster/strophe.roster.js"></script>

	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/rsm/strophe.rsm.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.6/mam/strophe.mam.v0.3.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/strophejs-plugins@0.0.7/archive/strophe.archive.js"></script>


	<!-- Vue.js development version, includes helpful console warnings -->
	<!-- <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script> -->

	<!-- <script src=""></script> -->
	<!-- Chart.js -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.min.js"></script>

	<!--W3 CSS-->
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" href="css/chatbox.css">
	<link rel="stylesheet" href="css/global.css">
	<link rel="stylesheet" href="css/navbar.css">
	<link rel="stylesheet" href="css/footer.css">
	<link rel="stylesheet" href="css/dashboard.css">
	<link rel="stylesheet" href="css/calendar.css">
</head>

<body>

	<!-- CHAT BOX -->
	<div class="w3-sidebar w3-bar-block w3-border-right" style="display:none;width:33%;right:0; height: 100%; background-color: transparent;"
	    id="chatBox">
		<div class="row chat-top">
		</div>
		<!--CHATBOX HEADER-->
		<button onclick="w3_close()" class="message-count" id="close-chat">
			<span class="fa fa-comment" id="close-chat-bubble">
			</span>
		</button>
		<div class="chat-header">
			<div class="row">
				<img src="img/ppapi.PNG" />
			</div>
			<div class="row" style="text-align: center;">
				<div class="col-lg-4"></div>
				<div class="col-lg-4">
					<p id="chatWithInfo">PRC Chat</p>
				</div>
				<div class="col-lg-4"></div>
			</div>
		</div>
		<!--MAIN CHAT CONTAINER-->
		<div class="main-chat-container" style="display:none">
			<!--CHAT MESSAGES-->
			<div class="row chat-messages">
				<div class="col-sm-12">
					<div class="chat-container">
						<div hidden class="row message-container conversant-message">
							<div class="col-sm-1">
								<img class="img-responsive" src="img/face.png">
							</div>
							<div class="col-sm-7 messages">
								<p class="message">asddddddddd</p>
							</div>
						</div>
						<div hidden class="row message-container user-message">
							<div class="col-sm-4"></div>
							<div class="col-sm-7 messages">
								<p class="message">asddddddddd</p>
							</div>
							<div class="col-sm-1">
								<img class="img-responsive" src="img/face.png">
							</div>
						</div>
					</div>
				</div>
			</div>
			<!--CHAT INPUT-->
			<div class="row chat-input-container">
				<div class="col-lg-12">
					<div class="chat-input">
						<textarea id="chat-input-text" placeholder="Type a message..."></textarea>
					</div>
				</div>
			</div>
		</div>
		<!-- CHAT CONTACT LIST -->
		<div class="chat-list-container row">
			<!-- CHAT SEARCH BAR -->
			<div class="row chat-search">
				<div class="col-sm-2">
					<i class="fa fa-search"></i>
				</div>
				<div class="col-sm-10">
					<textarea id="chat-search-box" placeholder="Search for a conversation"></textarea>
				</div>

			</div>
			<!-- CHAT CONTACTS -->
			<div hidden class=" contact-list">
				<ul id="contact-main-list">
					<li class="contact-item-container">
						<!-- COTNACT ITEM -->

						<div class="contact-item ">
							<!-- TIME -->
							<div class="row">
								<div class="col-sm-9">
								</div>
								<div class="col-sm-3 time">
									<p>4:15pm</p>
								</div>

							</div>
							<!-- INFO -->
							<div class="row">
								<!-- ICON -->
								<div class="col-sm-4">
									<img class="img-responsive" src="img/face.png">
								</div>
								<!-- DETAILS -->
								<div class="col-sm-8">
									<!-- NAME -->
									<div class="row contact-name">
										<p>Caloocan Social Hygene Clinic</p>
									</div>
									<!-- LAST MESSAGE -->
									<div class="row contact-message">
										<p>Kamusta naman yung mga...</p>
									</div>
								</div>
							</div>
						</div>
					</li>
				</ul>


			</div>
		</div>
	</div>

	<nav class="navbar navbar-expand-lg navbar-light">
		<a class="navbar-brand" href="#">
			<img class="img-responsive" src="img/logo.png" height=50 width=150>
		</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
		    aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>

		<div class="collapse navbar-collapse" id="navbarSupportedContent">
			<ul class="navbar-nav mr-auto">
				<li id="whatIsPrc" class="nav-item-custom">
					<span href="#">WHAT IS PRC</span>
				</li>
				<li id="announcements" class="nav-item-custom">
					<span href="#">ANNOUNCEMENTS</span>
				</li>
				<li id="database" class="nav-item-custom">
					<span href="#">DATABASE</span>
				</li>
				<li id="feedback" class="nav-item-custom">
					<span href="#">FEEDBACK</span>
				</li>
			</ul>
			<ul class="navbar-nav">
				<li>
					<form class="form-inline search-bar-custom">
						<input type="search" placeholder="Search" aria-label="Search">
					</form>
				</li>
				<li>
					<div class="account">
						<img class="img-responsive" src="img/face.png" height=55 width=55>
						<span class="fa fa-caret-down" data-toggle="dropdown"></span>
						<div class="dropdown account-dropdown">
							<ul class="dropdown-menu account-dropdown-menu">
								<li id="prc-info" type="button" data-toggle="modal" data-target="#profileModal">
									<span class="profile-edit">PRC</span>
								</li>
								<li>
									<span>Settings</span>
								</li>
								<li onclick="signout()">
									<span>Sign Out</span>
								</li>
							</ul>
						</div>
					</div>
				</li>
				<li>
					<div class="message-count">
						<a class="w3-right" onclick="w3_open()">
							<span class="fa fa-comment">
								<span class="message-count-text link-glow">5</span>
							</span>
						</a>
					</div>
				</li>
			</ul>

		</div>

		<div class="modal fade" id="profileModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog modal-lg" role="document">
				<div class="modal-content">
					<div class="modal-header">
						<h5 class="modal-title" id="exampleModalLabel">Profile</h5>
						<button type="button" class="close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<div class="modal-body">
						<div class="container">
							<div class="row">
								<div class="col-lg-2">
									<img class="img-responsive" src="img/ppapi.png" height=100 width=100>
								</div>
								<div class="col-lg-10">
									<div class="content">
										<div class="row row-nobottom-margin">
											<div class="col-lg-12">
												<div id="edit" class="action data">
													<span class="fa fa-pencil icon"> </span>
													<span class="label">Edit</span>
												</div>
												<div id="done" class="action data-edit hide">
													<span class="fa fa-check icon"> </span>
													<span class="label">Done</span>
												</div>
											</div>
										</div>
										<form action="">
											<div class="form-group">
												<div class="row">
													<div class="col-lg-12">
														<div class="title">
															<div class="data">
																<span id="titleView">PLHIV Response Center (PRC)</span>
															</div>
															<div class="data-edit hide">
																<input type="text" class="form-control" id="titleEdit" placeholder="Enter organization name">
															</div>
														</div>
													</div>
												</div>
												<div class="row">
													<div class="col-lg-12">
														<p id="adminLabel">PRC Administrators</p>
														<div class="admins data">
															<div id="admin-list" class="names">
																<span>Richard Bragado</span>
																<span>Lean Aseron</span>
																<span>Noel Quinto</span>
																<span>Rommel Legwes</span>
																<span>Benjamin Rivera</span>
																<span>Kai Ledesma</span>
															</div>
														</div>
														<div class="admins data-edit hide">
															<input type="hidden" name="count" value="1" />
															<div class="control-group admin-fields">
																<div class="controls">
																	<div class="input-append">
																		<div id="admin-field">
																			<input autocomplete="off" class="input form-control" id="admin-field1" name="prof1" type="text" placeholder="Enter admin name"
																			    data-items="8" />
																			<button id="b1" class="btn add-more" type="button">+</button>
																		</div>
																	</div>
																	<br>
																</div>
															</div>
														</div>
													</div>
												</div>
												<div class="row">
													<div class="col-lg-12">
														<div class="contact-info">
															<p>Contact Information</p>
															<div class="row">
																<div class="col-lg-1">
																	<img class="img-responsive icon" src="img/phone.png" height=30 width=30>
																</div>
																<div class="col-lg-11">
																	<div class="contact-num">
																		<div class="data">
																			<span id="numberView">(02) 743 7293</span>
																		</div>
																		<div class="data-edit hide">
																			<input type="tel" class="form-control" id="numberEdit" placeholder="Enter contact number">
																		</div>
																	</div>
																</div>
															</div>
															<div class="row">
																<div class="col-lg-1">
																	<img class="img-responsive icon" src="img/email.png" height=30 width=35>
																</div>
																<div class="col-lg-11">
																	<div class="contact-email">
																		<div class="data">
																			<span id="emailView">plhiv.response@gmail.com</span>
																		</div>
																		<div class="data-edit hide">
																			<input type="email" class="form-control" id="emailEdit" placeholder="Enter contact email">
																		</div>
																	</div>
																</div>
															</div>
															<div class="row">
																<div class="col-lg-1">
																	<img class="img-responsive icon" src="img/location.png" height=40 width=25>
																</div>
																<div class="col-lg-11">
																	<div class="contact-address">
																		<div class="data">
																			<span id="addressView">1965-B JRB Building, Anacleto Street, Sta. Cruz, Manila, Metro Manila</span>
																		</div>
																		<div class="data-edit hide">
																			<input type="text" class="form-control" id="addressEdit" placeholder="Enter contact address">
																		</div>
																	</div>
																</div>
															</div>
														</div>
													</div>
												</div>
											</div>
										</form>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</nav>

	<div class="container container-with-spacing">
		<div class="row">
			<div class="col-lg-8">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="red-rect-bg welcome">
							<p class="welcome-text">"Welcome back, PLHIV Response Center"</p>
							<p class="welcome-msg-count">You have

								<b class="link-glow" onclick="w3_open()">5</b>
								new messages</p>
							<!-- Welcom back caloocan city -->
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-lg-6">
						<div class="row row-with-padding">
							<div class="col-lg-12">
								<div class="white-rect-bg">
									<div class="container">
										<div class="row">
											<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
												<p class="box-label">Calendar &amp; Events</p>
												<div id="my-calendar"></div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="event-list">
						</div>
					</div>
					<div class="col-lg-6">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Cases Status Report</p>
										<p class="chart-title">Referred Services</p>
										<canvas id="refServicesChart" height="170vh"></canvas>
										<p class="chart-title">Number of Cases</p>
										<canvas id="numCasesChart" height="177"></canvas>
									</div>
								</div>
							</div>
						</div>
						<!-- case status report -->
					</div>
				</div>
			</div>
			<div class="col-lg-4">
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Latest Updates</p>
										<div class="update-list">
										</div>
									</div>
								</div>
							</div>

						</div>
					</div>
				</div>
				<div class="row row-with-padding">
					<div class="col-lg-12">
						<div class="white-rect-bg">
							<div class="container">
								<div class="row">
									<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
										<p class="box-label">Announcement Notifications</p>
										<div class="announcement-list">
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<footer class="page-footer">
		<div class="container-fluid">
			<div class="row">
				<div class="col-sm-2">
					<div class="contact-info">
						<div class="emails">
							<span>plhiv.response@gmail.com</span>
							<span>pinoy.plus@yahoo.com</span>
						</div>
						<div class="number">
							<span>(02) 743 7293</span>
						</div>
						<div class="address">
							<span>1965-B JRB Building, Anacleto Street, Sta Cruz, Manila</span>
						</div>
					</div>
				</div>
				<div class="col-sm-7"></div>
				<div class="col-sm-3">
					<div class="row">
						<div class="col-sm-6">
							<span>Follow us on our social media accounts</span>
						</div>
						<div class="col-sm-2">
							<a href="https://twitter.com/PlusAdvocacy">
								<img class="img-responsive twitter" src="img/twitter.png" height=55 width=60>
							</a>
						</div>
						<div class="col-sm-1"></div>
						<div class="col-sm-2">
							<a href="https://www.facebook.com/PinoyPlusAssociationInc/">
								<img class="img-responsive facebook" src="img/fb.png" height=50 width=35>
							</a>
						</div>
					</div>
					<div class="row">
						<div class="col-sm-12">
							<div class="copyright">
								<p class="copyright-text">Pinoy Plus Advocacy Pilipinas Inc. &copy 2018</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</footer>

	<input hidden id="username" value="<%= username %>">
	<input hidden id="password" value="<%= password %>">
	<input hidden id="adminAccount" value="<%= adminAccount %>">
	<input hidden id="adminServerUrl" value="<%= adminServerUrl %>">
	<input hidden id="chatServerUrl" value="<%= chatServerUrl %>">
	<input hidden id="chatHost" value="<%= chatHost %>">
</body>


<script src="js/keycodes.js"></script>
<script src="js/admin.js"></script>
<script src="js/dashboard-chat.js"></script>
<script src="js/edit-profile.js"></script>
<script src="js/calendar.js"></script>
<script src="js/dashboard.js"></script>
<script src="js/charts.js"></script>
<script src="js/messagebox.js"></script>
<script src="js/navbar.js"></script>

</html>